/* pages/resule/index.wxss */
.result{
  display: flex;
  flex-direction:column;
}

.result-left{
  display: inline-block;
  padding:5px 0;
  position:fixed;
  background-color:white;
  z-index:2;
  width:100%;
  box-shadow: 0 1px 8px 0 rgba(0,0,0,0.4);
}
.result-left-position{
  /* padding-top: 30rpx; */
  display:flex;
  /* height:40px; */
  flex-direction:row;
  justify-content: space-between;

}

.resule-left-list{
  font-size:8pt;
  color:#333333;
  letter-spacing:0px;
  padding:0 5px;
  margin:auto;

  /* margin:0 auto 53rpx 28rpx; */
}

.resule-left-listclick{
  color:#C8372B;
  font-size:8pt;
  letter-spacing:0px;
  padding:0 5px;
}

.result-right{
  margin-top:25px;
  display: inline-block;
  width: 100%;
  height:100vh;
  overflow-y:auto;
}
.brands{
  width:135rpx;
  height:80rpx;
  position:absolute;
  right:0;
}
.result-right-list{
  position: relative;
  padding: 40rpx 16rpx 0;
}
.nav-show{
 display: flex;
 align-items: center;
}
.nav-show .nav-show-block{
  width: 10rpx;
  height: 28rpx;
  background-color: #333333;
}
.nav-show text{
  font-size: 14px;
  color: #333333;
  letter-spacing: 0px;
  margin-left: 8rpx;
}

/*分别处理  基础信息 */

.result-right-message .list-content{
  display:flex;
  width:100%;
  overflow:hidden;
  padding:24rpx 0 0 0;
}

.list-left{
  font-size:14px;
  color:#999999;
  letter-spacing:0px;
  margin-right: 5px;
  display: flex;
  /* align-items: center;
  justify-content: center; */
  word-break: break-all;
  width: 30%;
}

.list-right{
  font-size:14px;
  color:#333333;
  letter-spacing:0px;
  display: flex;
  /* align-items: center; */
  /* justify-content: center; */
  word-break: break-all;
  width: 70%;
}

.result-right-technology .list-left{
  width: 100%;
}

/*统一样式 父级处理  */
.result-right-listcontent,.result-right-listtech{
  background:#FFFFFF;
  border:1rpx solid #D8D8D8;
  display:flex;
  flex-direction:column;
  padding:16rpx 0;
  margin-top:20rpx;
}
.result-right-listcontent .list-content{
  display:flex;
  width:94%;
  margin:4rpx auto;
  overflow: hidden;
}

/*技术信息  */
.result-right-technology .list-content{
  display: flex;
}

.result-right-listtech{
  border: 0;
  margin: 0;
}

.result-right-listtech .list-content:nth-child(5n){
  margin-top: 20rpx;
}

.notshow{
  display: none;
}